{
  "name": "react-authnet-iframe",
  "version": "1.0.0",
  "description": "A lightweight, modern React implementation of Authorize.net's Accept.JS library with embedded iframe functionality",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "authorize.net",
    "payment",
    "iframe",
    "accept.js",
    "credit-card",
    "payment-processing"
  ],
  "author": "Sergei Morugin",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "rollup": "^3.25.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "styled-components": "^6.0.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergeyMorugin/react-authnet-iframe.git"
  },
  "bugs": {
    "url": "https://github.com/SergeyMorugin/react-authnet-iframe/issues"
  },
  "homepage": "https://github.com/SergeyMorugin/react-authnet-iframe#readme"
}
